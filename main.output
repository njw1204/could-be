    8 standard_type: "int"
    9              | "float"

   10 subprogram_declarations: subprogram_declaration subprogram_declarations
   11                        | /* empty */

   12 subprogram_declaration: subprogram_head declarations compound_statement

   13 subprogram_head: "function" "identifier" arguments ':' standard_type ';'
   14                | "procedure" "identifier" arguments ';'

   15 arguments: '(' parameter_list ')'
   16          | /* empty */

   17 parameter_list: identifier_list ':' type
   18               | identifier_list ':' type ';' parameter_list

   19 compound_statement: "begin" statement_list "end"

   20 statement_list: statement
   21               | statement ';' statement_list

   22 statement: variable '=' expression
   23          | print_statement
   24          | procedure_statement
   25          | compound_statement
   26          | if_statement
   27          | while_statement
   28          | for_statement
   29          | "return" expression
   30          | "nop"

   31 if_statement: "if" expression ':' statement
   32             | "if" expression ':' statement "else" ':' statement
   33             | "if" expression ':' statement elif_statement
   34             | "if" expression ':' statement elif_statement "else" ':' statement

   35 elif_statement: "elif" expression ':' statement elif_statement
   36               | "elif" expression ':' statement

   37 while_statement: "while" expression ':' statement
   38                | "while" expression ':' statement "else" ':' statement

   39 for_statement: "for" in_expression ':' statement
   40              | "for" in_expression ':' statement "else" ':' statement

   41 print_statement: "print"
   42                | "print" '(' expression ')'

   43 variable: "identifier"
   44         | "identifier" '[' expression ']'

   45 procedure_statement: "identifier" '(' actual_parameter_expression ')'

   46 actual_parameter_expression: expression_list
   47                            | /* empty */

   48 expression_list: expression
   49                | expression ',' expression_list

   50 expression: simple_expression
   51           | in_expression
   52           | simple_expression relop simple_expression

   53 in_expression: simple_expression "in" simple_expression

   54 simple_expression: term
   55                  | term addop simple_expression

   56 term: factor
   57     | factor multop term

   58 factor: "integer number"
   59       | "decimal number"
   60       | variable
   61       | procedure_statement
   62       | '!' factor
   63       | sign factor

   64 sign: '+'
   65     | '-'

   66 relop: ">"
   67      | ">="
   68      | "<"
   69      | "<="
   70      | "=="
   71      | "!="

   72 addop: '+'
   73      | '-'

   74 multop: '*'
   75       | '/'


